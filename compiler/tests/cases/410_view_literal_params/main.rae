func takeViewString(s: view String) {
  log("view {s}")
}

func takeViewInt(i: view Int) {
  log("view {i}")
}

func takeViewFloat(f: view Float) {
  log("view {f}")
}

func takeViewBool(b: view Bool) {
  log("view {b}")
}

func main() {
  takeViewString("Literal String")
  takeViewInt(42)
  takeViewFloat(3.14)
  takeViewBool(true)
}
