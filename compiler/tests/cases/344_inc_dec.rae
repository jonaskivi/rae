type Counter {
  val: Int
}

func main() {
  def x: Int = 10
  log(value: ++x) # 11
  log(value: x)   # 11
  log(value: x++) # 11
  log(value: x)   # 12
  
  log(value: --x) # 11
  log(value: x)   # 11
  log(value: x--) # 11
  log(value: x)   # 10
  
  def c: Counter = {val: 5}
  log(value: ++c.val) # 6
  log(value: c.val)   # 6
  log(value: c.val++) # 6
  log(value: c.val)   # 7
  
  log(value: --c.val) # 6
  log(value: c.val)   # 6
  log(value: c.val--) # 6
  log(value: c.val)   # 5
}
