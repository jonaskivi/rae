type Counter {
  val: Int
}

func main() {
  def x: Int = 10
  # 11
  # 11
  log(++x)
  # 11
  log(x)
  # 12
  log(x++)
  # 11
  log(x)
  # 11
  log(--x)
  # 11
  log(x)
  # 10
  log(x--)
  log(x)
  def c: Counter = { val: 5 }
  # 6
  # 6
  log(++c.val)
  # 6
  log(c.val)
  # 7
  log(c.val++)
  # 6
  log(c.val)
  # 6
  log(--c.val)
  # 6
  log(c.val)
  # 5
  log(c.val--)
  log(c.val)
}
