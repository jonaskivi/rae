type Counter {
  val: Int
}

func main() {
  def x: Int = 10
  log(++x) # 11
  log(x)   # 11
  log(x++) # 11
  log(x)   # 12
  
  log(--x) # 11
  log(x)   # 11
  log(x--) # 11
  log(x)   # 10
  
  def c: Counter = {val: 5}
  log(++c.val) # 6
  log(c.val)   # 6
  log(c.val++) # 6
  log(c.val)   # 7
  
  log(--c.val) # 6
  log(c.val)   # 6
  log(c.val--) # 6
  log(c.val)   # 5
}
