func add(a: Int, b: Int) ret Int {
  ret a + b
}

func triple(value: Int) ret Int {
  ret value * 3
}

func chain(a: Int, b: Int, c: Int) ret Int {
  ret triple(value: a + b) + c
}

func main() {
  log("VM math demo start")
  log(add(a: 2, b: 5))
  log(triple(value: 4))
  log(chain(a: 1, b: 2, c: 3))
  log("VM math demo end")
}
