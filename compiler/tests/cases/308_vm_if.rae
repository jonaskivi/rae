func main() {
  if 1 < 2 {
    log(value: "then")
  } else {
    log(value: "else")
  }

  if 5 < 3 {
    log(value: "nope")
  } else {
    log(value: "else branch")
  }

  def total: Int = 2 + 3
  if total > 4 and 1 < 2 {
    log(value: "and ok")
  }

  log(value: 1 < 2 and 0 < 1)
  log(value: 5 < 2 or 3 < 1)
  log(value: not (1 < 2))
}
