func doubleValue(value: Int) {
  helper(input: value * 2)
}

func helper(input: Int) {
  log(value: "Helper ran")
}

func sumChain(a: Int, b: Int, c: Int) {
  log(value: "sumChain start")
  doubleValue(value: a + b + c)
  log(value: "sumChain done")
}

func main() {
  log(value: "C backend demo start")
  sumChain(a: 1, b: 2, c: 3)
  log(value: "C backend demo end")
}
