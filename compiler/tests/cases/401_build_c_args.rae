func helper(a: Int, b: Int) {
  log(value: "Helper ran")
}

func sumChain(a: Int, b: Int): ret Int {
  log(value: "sumChain start")
  def total: Int = a + b
  if total > 5 {
    log(value: "total > 5")
  } else {
    log(value: "total <= 5")
  }
  log(value: "sumChain done")
  ret total
}

func main() {
  log(value: "C backend demo start")
  helper(a: 1, b: 2)
  def res: Int = sumChain(a: 3, b: 4)
  log(value: "C backend demo end")
}