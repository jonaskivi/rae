type State {
  score: Int
  active: Bool
  name: String
}

func main() {
  # score: 0, active: false, name: ""
  let s: State
  if s.score is 0 {
    log("score ok")
  }
  if s.active is false {
    log("active ok")
  }
  if s.name is "" {
    log("name ok")
  }
  let x: Int
  if x is 0 {
    log("primitive ok")
  }
  if s.active.toString() is "false" {
    log("bool toString ok")
  }
}
