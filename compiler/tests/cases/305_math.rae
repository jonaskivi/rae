func add(a: Int, b: Int): ret Int {
  ret a + b
}

func triple(value: Int): ret Int {
  ret value * 3
}

func chain(a: Int, b: Int, c: Int): ret Int {
  ret triple(value: a + b) + c
}

func main() {
  log(value: "VM math demo start")
  log(value: add(a: 2, b: 5))
  log(value: triple(value: 4))
  log(value: chain(a: 1, b: 2, c: 3))
  log(value: "VM math demo end")
}