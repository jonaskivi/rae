func main() {
  log(value: "--- Random Basic Example ---")
  
  # Set a seed for deterministic results
  seed(n: 123)
  log(value: "Seed set to 123")

  log(value: "Three random floats (0.0 to 1.0):")
  log(value: random())
  log(value: random())
  log(value: random())

  log(value: "Three random integers (1 to 100):")
  log(value: randomInt(min: 1, max: 100))
  log(value: randomInt(min: 1, max: 100))
  log(value: randomInt(min: 1, max: 100))
}
