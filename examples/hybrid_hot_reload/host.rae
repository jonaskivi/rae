func main() {
  def step: Int = 0
  loop step < 5 {
    log(value: "[Hybrid demo] Loop tick")
    log(value: step)
    step = step + 1
    if step is 2 {
      log(value: "[Hybrid demo] Dev hot-reload bundle staged")
    }
    log(value: "[Hybrid demo] Release bundle staged")
  }
  log(value: "[Hybrid demo] Loop exit")
}

func init() {
  log(value: "[Hybrid demo] Booting hybrid host runtime")
}

func shutdown() {
  log(value: "[Hybrid demo] Hybrid host shutdown")
}