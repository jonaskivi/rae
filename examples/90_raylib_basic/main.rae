import raylib
func main() {
  initWindow(width: 800, height: 450, title: "Raylib Basic")
  let c: Color = { r: 255, g: 255, b: 255, a: 255 }
  loop windowShouldClose() is 0 {
    beginDrawing()
    clearBackground(color: c)
    drawCircle(
      x: 400.0
      y: 225.0
      radius: 50.0
      color: { r: 255, g: 0, b: 0, a: 255 }
    )
    endDrawing()
  }
  closeWindow()
}
