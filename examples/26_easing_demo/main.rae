import easing

func main() {
  log("Easing Demo (t from 0.0 to 1.0):")
  
  let i: Int = 0
  loop i <= 10 {
    let t: Float = i.toFloat() / 10.0
    
    let linear: Float = easeLinear(t)
    let sineIn: Float = easeInSine(t)
    let quadOut: Float = easeOutQuad(t)
    let bounceOut: Float = easeOutBounce(t)
    
    log("t: {t} -> Linear: {linear}, SineIn: {sineIn}, QuadOut: {quadOut}, BounceOut: {bounceOut}")
    
    i = i + 1
  }
}
