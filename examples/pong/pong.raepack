pack Pong {
  format: "raepack"
  version: 1
  defaultTarget: live
  targets: {
    target live: {
      label: "Live"
      entry: "main.rae"
      sources: {
        source: { path: ".", emit: live }
      }
    }
    target compiled: {
      label: "Compiled"
      entry: "main.rae"
      sources: {
        source: { path: ".", emit: compiled }
      }
    }
    target hybrid: {
      label: "Hybrid"
      entry: "main.rae"
      sources: {
        source: { path: ".", emit: hybrid }
      }
    }
  }
}
