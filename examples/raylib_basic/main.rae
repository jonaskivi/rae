import raylib
func game_frame() {
  beginDrawing()
  clearBackground(
    r: 255,
    g: 255,
    b: 255,
    a: 255,
  )
  drawText(
    text: "Hello from Rae + Raylib!",
    x: 190,
    y: 200,
    fontSize: 20,
    r: 200,
    g: 200,
    b: 200,
    a: 255,
  )
  endDrawing()
}

func main() {
  initWindow(width: 800, height: 450, title: "Rae Raylib Basic")
  loop windowShouldClose() is 0 {
    game_frame()
  }
  closeWindow()
}
