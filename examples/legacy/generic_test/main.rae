type Box(T) {
  value: T
}

func setValue(T)(b: mod Box(T), val: T) {
  b.value = val
}

func main() {
  # Int Box
  def b: Box(Int) = { value: 42 }
  log(b.value)
  # Float Box
  setValue(b, val: 100)
  log(b.value)
  def bf: Box(Float) = { value: 3.14 }
  log(bf.value)
  # String Box
  setValue(bf, val: 2.718)
  log(bf.value)
  def bs: Box(String) = { value: "hello" }
  log(bs.value)
  setValue(bs, val: "world")
  log(bs.value)
}


