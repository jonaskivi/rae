func main() {
  log("Standard List Test")
  def list: List(Any) = {
    10
    "Hello"
    true
    3.14
    }
  log("List items:")
  def i: Int = 0
  loop i < list.length() {
    log(list[i])
    i = i + 1
  }
  log("Modification:")
  list.add(value: "New Item")
  loop i < list.length() {
    log(list[i])
    i = i + 1
  }
  log("Done.")
}


