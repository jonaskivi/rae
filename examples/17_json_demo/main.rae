type Point {
  x: Int
  y: Int
  name: String
}

func main() {
  # 1. To JSON
  let p: Point = { x: 10, y: 20, name: "Home" }
  let json: String = p.toJson()
  log("Point JSON: {json}")

  # 2. From JSON
  let p2: Point = Point.fromJson(json: json)
  log("Point2 x: {p2.x}")
  log("Point2 y: {p2.y}")
  log("Point2 name: {p2.name}")
}
