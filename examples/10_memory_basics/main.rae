type Point {
  x: Int
  y: Int
}

func main() {
  # 1. Value Type
  let p1: Point = { x: 10, y: 20 }
  # Copy
  let p2: Point = p1
  p2.x = 30
  # Prints 10
  # 2. Borrow Type
  log(p1.x)
  let r: mod Point => p1
  r.y = 50
  # Prints 50
  # 3. Identity Types
  log(p1.y)
  let pid: id Point = 1
  let pkey: key Point = "point_001"
  # 4. Borrow
  log(pid)
  log(pkey)
  let r2: view Point => p1
  log(r2.x)
}
