import time

func main() {
  let t: Timer = createTimer()
  
  log("Starting timer...")
  start(t)
  
  log("Sleeping for 1 second...")
  sleep(1000)
  
  stop(t)
  
  let ns: Int = elapsedNs(t)
  let ms: Float = elapsedMs(t)
  let s: Float = elapsedSeconds(t)
  
  log("Elapsed ns: {ns}")
  log("Elapsed ms: {ms}")
  log("Elapsed s: {s}")
  
  reset(t)
  log("Elapsed after reset: {elapsedNs(t)}")
}
