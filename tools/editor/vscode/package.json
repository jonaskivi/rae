{
  "name": "rae-lang",
  "displayName": "Rae Language",
  "description": "Syntax highlighting for the Rae language",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rae",
        "aliases": [
          "Rae",
          "rae"
        ],
        "extensions": [
          ".rae",
          ".raepack"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rae",
        "scopeName": "source.rae",
        "path": "./syntaxes/rae.tmLanguage.json"
      }
    ]
  }
}
